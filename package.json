{
  "name": "n8n-nodes-postgres-context",
  "version": "1.0.2",
  "description": "n8n nodes for AI context management with PostgreSQL",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "ai",
    "context",
    "postgresql"
  ],
  "license": "MIT",
  "author": {
    "name": "Aron Peyroteo",
    "email": "aron@mqx.com.br"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && eslint -c .eslintrc.prepublish.js src",
    "test": "ts-node tests/database.test.ts"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/ContextManager/ContextManager.node.js",
      "dist/nodes/ExternalContext/ExternalContext.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "pg": "^8.8.0"
  }
}
